{
    "name": "Community BC",
    "title": "Community BC Changes",
    "description": "Community BC Changes",
    "services": [
        "Customizer"
    ],
    "state": "enabled",
    "extensions": [
        {
            "name": "CommunityBC",
            "type": "com.ibm.customizer.ui",
            "payload": {
